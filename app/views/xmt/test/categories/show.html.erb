<div class="page-header">
  <h1>查看分类信息<small></small></h1>
</div>
<p id="notice"><%= notice %></p>

<div class="row">
  <div class="col-sm-12">
    <table id="user" class="table table-bordered table-striped" style="clear: both">
      <tbody>
      <tr>
        <td class="column-left">分类名称：</td>
        <td class="column-right">
          <%= @category.title %>
        </td>
      </tr>
      </tbody>
    </table>

  </div>
</div>
<div class="panel-body">
  <div class="row">
    <div class="col-md-12 space20">

      <a class="btn btn-teal" href="<%= edit_xmt_test_category_path(@category)%>">
        <i class="fa fa-edit"></i>
      </a>
      <a class="btn btn-primary" href="<%= xmt_test_categories_path %>">
        <i class="fa fa-reply"></i>
      </a>
    </div>
  </div>

</div>
