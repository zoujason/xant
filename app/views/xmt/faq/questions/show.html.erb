<div class="page-header">
  <h1>查看问题内容<small></small></h1>
</div>
<p id="notice"><%= notice %></p>

<div class="row">
  <div class="col-sm-12">
    <table id="user" class="table table-bordered table-striped" style="clear: both">
      <tbody>
      <tr>
        <td class="column-left">title：</td>
        <td class="column-right">
          <%= @question.title %>
        </td>
      </tr>
      <tr>
        <td class="column-left">qty：</td>
        <td class="column-right">
          <%= @question.qty %>
        </td>
      </tr>
      <tr>
        <td class="column-left">seq：</td>
        <td class="column-right">
          <%= @question.seq %>
        </td>
      </tr>
      </tbody>
    </table>

  </div>
</div>
<div class="panel-body">
  <div class="row">
    <div class="col-md-12 space20">

      <a class="btn btn-teal" href="<%= edit_xmt_faq_question_path(@question)%>">
        <i class="fa fa-edit"></i>
      </a>
      <a class="btn btn-primary" href="<%= xmt_faq_questions_path%>">
        <i class="fa fa-reply"></i>
      </a>
    </div>
  </div>

</div>
