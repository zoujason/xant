  <style>
    /* 清除浮动 */
    .clear, .data_box, .info_box, ul, dl{ zoom:1}
    .clear:after, .data_box:after, .info_box:after, ul:after, dl:after{ content:''; display:block; clear:both;}





    /* 文本框 */
    input.text{ height: 22px; line-height:22px; border:1px solid #ddd; padding-left:5px;}





    /* 按钮.btn_gray_h30{ height: 30px; border: 1px solid #eee; background: url(../../images/trade/bnt_gray_h30.png) repeat-x; border-radius: 3px; -moz-border-radius: 3px; color:#333; cursor: pointer;} */
    .btn_yellow{ border: 1px solid #FF6401; background-color: #FF8700; border-radius: 3px; -moz-border-radius: 3px; color: #fff; cursor: pointer;}
    .btn_yellow:hover{ background-color: #FFA200;}


    /* box */
    .box_1{ border: 1px solid #ddd;}
    .box_1 .box1_title{ width: 100%; height: 30px; background-color: #f7f7f7; border-bottom: 1px solid #ddd; line-height: 30px; font-size: 14px; text-indent: 10px; position: relative;}
    .box_1 .box1_title .btn_gray, .box_1 .box1_title .btn_yellow{ width: 50px; height: 24px; margin-right: 15px; font-size: 12px;}
    /*.box_1 .box1_title .arrows{ width: 20px; height: 11px; background: url("../../images/trade/box1_title_arrows.png") no-repeat; position: absolute;}*/
    .box_1 .box1_title .more{ color: #004ea1; font-size: 12px; float: right; margin-right: 5px;}
    .box_2 .box2_title{ width: 100%; height: 30px; border-bottom: 1px solid #e8e8e8; line-height: 30px; font-size: 14px; text-indent: 10px; position: relative;}
    .box_2 .box2_title .operation{ height: 14px; position: absolute; top: 9px; left: 110px;}
    .box_2 .box2_title .operation a{ text-indent: 0; float: left; margin-right: 35px;}
    .box_2 .box2_title .more{ color: #004ea1; font-size: 12px; float: right; margin-right: 5px;}
    .box_2 .box2_content{ border: 1px solid #e8e8e8;}


    /* info_box */
    .info_box{ width: 100%; height: 24px;}
    .info_box .box_title{ height: 24px; line-height: 24px; text-align: right; float: left;}
    .info_box .box_text{ height: 24px; line-height: 24px; float: left;}
    .info_box .box_text, .box_text{ position: relative;}
    .info_box .box_text .address_span{ float: left; position: relative;}
    .info_box .box_text .address_span .error{ left: 3px;}
    .info_box .box180{ width: 180px; float: left;}
    .info_box .box180 .box_title{ width: 80px;}
    .info_box .box180 .box_text{ width: 100px;}
    .info_box .box253{ width: 253px; float: left;}
    .info_box .box253 .box_title{ width: 90px;}
    .info_box .box253 .box_text{ width: 163px;}
    .info_box .box260{ width: 260px; float: left;}
    .info_box .box260 .box_title{ width: 90px;}
    .info_box .box260 .box_text{ width: 170px;}
    .info_box .box285{ width: 285px; float: left;}
    .info_box .box285 .box_title{ width: 90px;}
    .info_box .box285 .box_text{ width: 195px;}
    .info_box .box310{ width: 310px; float: left;}
    .info_box .box310 .box_title{ width: 90px;}
    .info_box .box310 .box_text{ width: 220px;}
    .info_box .box332{ width: 332px; float: left;}
    .info_box .box332 .box_title{ width: 90px;}
    .info_box .box332 .box_text{ width: 242px;}
    .info_box .box370{ width: 370px; float: left;}
    .info_box .box370 .box_title{ width: 90px;}
    .info_box .box370 .box_text{ width: 310px;}
    .info_box .box425{ width: 425px; float: left;}
    .info_box .box425 .box_title{ width: 90px;}
    .info_box .box425 .box_text{ width: 335px;}
    .info_box .box465{ width: 465px; float: left;}
    .info_box .box465 .box_title{ width: 90px;}
    .info_box .box465 .box_text{ width: 375px;}
    .info_box .box499{ width: 499px; float: left;}
    .info_box .box499 .box_title{ width:179px;}
    .info_box .box499 .box_text{ width: 320px;}
    .info_box .box507{ width: 507px; float: left;}
    .info_box .box507 .box_title{ width: 60px;}
