    .info_box .box620{ width: 620px; float: left;}
    .info_box .box620 .box_title{ width: 90px;}
    .info_box .box620 .box_text{ width: 530px;}
    .info_box .box700{ width: 700px; float: left;}
    .info_box .box700 .box_title{ width: 90px;}
    .info_box .box700 .box_text{ width:610px;}
    .info_box .box750{ width: 750px; float: left;}
    .info_box .box750 .box_title{ width:150px;}
