    .info_box .box998 .box_title{ width: 179px;}
    .info_box .box998 .box_text{ width: 800px;}
    .info_box .box998 .long_text{margin-left:6px; height:24px; line-height:24px; border:1px solid #DDD; float:left;}

    .info_box input.text, .info_box .text_address, .info_box .text_date, .info_box .text_website, .info_box .spinner, .info_box .word, .info_box .file-box{ float: left;}
    .info_box .unit{ float: left; margin-left: 3px;}
    .info_box .star{ height: 24px; line-height: 27px; float: left;}
    .info_box .btn input{ width: 80px; height: 26px; float: left;}
    .info_box .box260.btn .btn_yellow{ margin: 0 20px 0 50px;}
    .info_box .box332.btn .btn_yellow{ margin: 0 20px 0 112px;}



    /* data_box */
    .data_title{height:29px;line-height:29px;background:#F7F7F7; border-bottom:1px solid #ddd;}
    .data_title span{float:left;color:#666; text-align:center;}
    /*.data_box span{float:left;color:#666; text-align:center;}*/
    .data_box div{float:left;}


    /* 数据列表鼠标经过变色 */
    .data_list .data_box.hover{ background-color: #DDEEFF;}
    /*没有数据时的样式*/
    .no_data{ height: 60px; line-height: 60px; text-align:center}

    /*超出内容部分用省略号*/
    .txt_apostrophe{word-break:keep-all;white-space:nowrap;overflow:hidden;text-overflow:ellipsis;}

    /* 列表无数据样式*/
    .data_list .nodata{ width: 100%; height: 45px; line-height: 45px; text-align: center; color: #999999; font-size: 12px;}
    .data_list .nodata.height60{ height: 60px;}
    .last_line_long{ width:100%; border:1px solid #DDD;overflow: hidden;}

    /*普通查询*/
    .advanced_select{display:none;}

    .main{ width: 1190px; margin: 10px auto 20px;}
    .advmsg_manage .box_1{ width: 998px;}
    .display_none{ display: none;}
    /*部门机构管理1*/
    .orgaManage1 .box1_title{color:#666; height:40px; line-height:40px;}
    .orgaManage1 .box1_title input{margin-left:10px;  border:1px solid #DDD;height:22px;line-height:22px; padding:0 10px;}
    .orgaManage1 .cont_list{width:97.4%; padding:22px 0 20px; margin:0 auto;}
    .orgaManage1 .cont_left{ width:202px; margin-right:7px; display:inline;float:left; height:736px;}
    .orgaManage1 .cont_box{ border:1px solid #DDD; overflow: hidden;}
    .orgaManage1 .cont_left .cont_box{ width: 200px; height:730px;overflow:auto;}
    .orgaManage1 .cont_title{height:36px;line-height:36px;background:#F7F7F7; padding-left:18px;}
    .orgaManage1 .cont_right{width:762px; float:left;}
    .orgaManage1 .cont_right .cont_box{ width: 760px; height:730px;}
    .orgaManage1 .cont_right .box2_title{ height:43px; line-height:54px;overflow:hidden; border-bottom:2px solid #ddd; width:95%; margin-left:26px;}
    .orgaManage1 .box2_content{padding-top:30px;}
    .orgaManage1 .info_box{ padding-bottom:30px;}
    .orgaManage1 .check span{ margin:0 15px;}
    .orgaManage1 .check input{cursor: pointer;margin: 5px 10px 0 0;}
    .orgaManage1 .text_address{ margin-right: 3px;}
    .orgaManage1 .btns{ padding: 0;}
    .orgaManage1 .box2_title input{ width: 50px; height: 24px; font-size: 12px; margin-right: 5px;}

    /*部门机构 弹框*/
    .grey_box.Branch1{ width: 800px; height: 570px;}
    .grey_box.Branch1 .info_box{ width: 780px; margin: auto; padding: 25px 0;}
    .grey_box.Branch1 .box_1{ width: 750px; margin: auto;}
    .grey_box.Branch1 .data_list .data_box span{ height: 35px; line-height: 35px;}
    .grey_box.Branch1 .data1{ width: 140px;}
    .grey_box.Branch1 .data2{ width: 500px;}
    .grey_box.Branch1 .data3{ width: 320px;}
